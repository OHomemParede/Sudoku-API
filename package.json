{
    "name": "sudoku-api",
    "version": "0.0.0",
    "repository": "https://github.com/MatheusIshiyama/Sudoku-API.git",
    "author": "Matheus Ishiyama <semrumo3@hotmail.com> & Daniel de Souza Lima Junior <Dewaldman@Outlook.com>",
    "license": "MIT",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
        "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
        "clean": "rmdir /s .\\dist || rm -rf ./dist"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "cors": "^2.8.5"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.5",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0",
        "tscpaths": "^0.0.9",
        "typescript": "^4.4.3"
    },
    "engines": {
        "node": ">=14.17.4"
    }
}
